    "lit-element": "^2.5.1",
    "rollup": "^2.39.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-filesize": "^9.1.0",
    "snowpack": "^3.7.1",
    "sort-package-json": "^1.52.0",
    "static-params": "^0.2.1",
    "uvu": "^0.5.1"
