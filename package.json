{
  "name": "mint-element",
  "version": "0.0.1",
  "description": "Create CustomElements using observables.",
  "keywords": [
    "custom-elements",
    "observables",
    "frp",
    "currying",
    "fp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/danny-andrews/mint-element/"
  },
  "license": "MIT",
  "author": "Danny Andrews <lunde.andrews@gmail.com> (dannyandrews.net)",
  "sideEffects": false,
  "type": "module",
  "module": "./src/index.js",
  "browser": "./src/index.js",
  "files": [
